<decoder name="industrial-honeypot-syslog">
    <!--
    Log examples:
    <14>1 2020-01-01T00:00:00.000000+00:00 5ff0a7039b2e conpotjsonlog - - [timeQuality tzKnown="1" isSynced="0"] {"timestamp": "2020-01-01T00:00:00.000000", "sensorid": "conpot", "id": "f83ec551-308a-4dd6-afe7-a545d23ce1cb", "src_ip": "172.21.0.8", "src_port": 45740, "dst_ip": "172.21.0.6", "dst_port": 161, "public_ip": "1.2.3.4", "data_type": "snmp", "request": {"oid": "1.3.6.1.2.1.1.5.0", "val": ""}, "response": {"oid": "1.3.6.1.2.1.1.5.0", "val": "Pump Control Unit"}, "event_type": "SNMPv2 Get"}
    -->
    <prematch type="pcre2">.*conpotjsonlog - - \[.+\] </prematch>
    <plugin_decoder offset="after_prematch">JSON_Decoder</plugin_decoder>
    <json_null_field>discard</json_null_field>
</decoder>

<decoder name="industrial-honeypot-suricata-syslog">
    <prematch type="pcre2">.*suricatajsonlog - - \[.+\] </prematch>
    <plugin_decoder offset="after_prematch">JSON_Decoder</plugin_decoder>
</decoder>
